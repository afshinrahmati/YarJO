/////////////////////////////////////////////////////// carosel slider


function showtog(a){
  var b = document.getElementById(a);
  if(b.className == "fa fa-plus"){
    b.className = "fa fa-minus";
    b.parentNode.style.backgroundColor="#ffffff";
    b.parentNode.style.color="black";

  }
  else{
    b.className = "fa fa-plus";
    b.parentNode.style.backgroundColor="#f3f3f3";
    b.parentNode.style.color="black";
  }
}


